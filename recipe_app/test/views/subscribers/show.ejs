<h1 class="center">Student Data for
  <%= subscriber.name %>
</h1>

<table class="table">
  <tr>
    <th>Name</th>
    <td>
      <%= subscriber.name %>
    </td>
  </tr>
  <tr>
    <th>CSC141</th>
    <td>
      <%= subscriber.grade1%>
    </td>
  </tr>
  <tr>
    <th>CSC142</th>
    <td>
      <%= subscriber.grade2%>
    </td>
  </tr>
  <tr>
    <th>CSC240</th>
    <td>
      <%= subscriber.grade3%>
    </td>
  </tr>
  <tr>
    <th>CSC241</th>
    <td>
      <%= subscriber.grade4%>
    </td>
  </tr>
</table>


<div>
  <a class="button" href="/subscribers">View all Students</a>
</div>
<div>
  <a class="button" href="<%=`/subscribers/${subscriber._id}/edit`%>">
    Edit Student Details
  </a>
</div>
<div>
  <a class="button" href="<%= `/subscribers/${subscriber._id}/delete?_method=DELETE` %>" onclick="return confirm('Are you sure you want to delete this record?')">Delete</a>
</div>
